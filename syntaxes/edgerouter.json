    "line-address-with-subnet":{
      "patterns":[
        {
          "begin":"(address)\\s",
          "beginCaptures":{
            "1":{"name":"keyword.other.edgemax"}
          },
          "end":"$",
          "patterns":[
            {
              "include":"#part-address-ipv4-with-subnet"
            }
          ]
        }
      ]
    },
    "line-defaultrouter-address":{
