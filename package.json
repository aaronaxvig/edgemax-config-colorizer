{
    "name": "edgemax-config-colorizer",
    "displayName": "EdgeMax Config Colorizer",
    "description": "Syntax highlighting for Ubiquiti's EdgeRouter and EdgeSwitch configuration files.",
    "version": "0.0.1",
    "publisher": "aaronaxvig",
    "engines": {
        "vscode": "^1.4.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "edgerouter",
                "aliases": ["EdgeRouter", "edgerouter"],
                "extensions": [".edgerouter"],
                "configuration": "./language-configuration.json"
            },
            {
                "id": "edgeswitch",
                "aliases": ["EdgeSwitch", "edgeswitch"],
                "extensions": [".edgeswitch"],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "edgerouter",
                "scopeName": "text.edgerouter",
                "path": "./syntaxes/edgerouter.json"
            },
            {
                "language": "edgeswitch",
                "scopeName": "text.edgeswitch",
                "path": "./syntaxes/edgeswitch.json"
            }
        ]
    }
}